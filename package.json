{
  "name": "pine",
  "version": "1.0.0",
  "description": "househunt",
  "main": "lib/index.js",
  "dependencies": {
    "@babel/core": "^7.7.5",
    "express": "^4.17.1",
    "mongoose": "^5.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "eslint": "^6.7.2",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1"
  },
  "scripts": {
    "test": "npm run build && nyc mocha lib/components/users/userTests",
    "debug": "babel-node --inspect-brk=9229 src/index.js",
    "dev": "nodemon --exec babel-node -- bin/www",
    "prod": "node lib/index.js",
    "build": "babel src -d lib"
  },
  "mocha": {
    "reporter": "spec",
    "exit": "true"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "all": "true",
    "exclude": [
      "src",
      "public",
      "coverage"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ceelogre/pine.git"
  },
  "keywords": [
    "house",
    "rent",
    "broker"
  ],
  "author": "Pine",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ceelogre/pine/issues"
  },
  "homepage": "https://github.com/ceelogre/pine#readme"
}
